year <- sub("-.*", "", meta$Date)
note <- sprintf("R package version %s", meta$Version)

bibentry(bibtype = "Manual",
         title = "extraclassical: Models And Explains Extraclassical Field Effects",
         author = c(person("Samuel", "Lippl")),
         year = "2019",
         note = note,
         url = "https://github.com/sflippl/extraclassical")
